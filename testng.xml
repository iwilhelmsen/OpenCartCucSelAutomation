<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="E2E Cucumber Feature Test" parallel="tests" thread-count="10">
    <listeners>
        <listener class-name="ProjectListener"/>
    </listeners>
    <parameter name="suite-browser" value="chrome"/>
    <test name="Register Feature">
        <classes>
            <class name="runners.registerTestRunner"/>
            <class name="runners.loginTestRunner"/>
        </classes>
    </test>
</suite>